{
  "name": "attocode",
  "version": "1.0.0",
  "description": "Standalone production coding agent extracted from first-principles-agent",
  "type": "module",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/main.ts",
    "start": "tsx src/main.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "chalk": "^5.3.0",
    "dotenv": "^17.2.3",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "react": "^18.2.0",
    "terminal-image": "^2.0.0",
    "typescript-language-server": "^5.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.25.0",
    "openai": "^4.20.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "agent",
    "llm",
    "coding-assistant",
    "attocode"
  ]
}
